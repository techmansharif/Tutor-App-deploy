{
  "hosting": {
    "public": "build",
    "ignore": [
      "firebase.json",
      "*/.",
      "*/node_modules/*"
    ],
    "redirects": [
      {
        "source": "/tutorssc",
        "destination": "https://brimai-test-v1.web.app/",
        "type": 301
      }
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}